<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="439.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.VehicleManageFormController">
   <children>
      <TextField fx:id="txtUserId" layoutX="38.0" layoutY="48.0" onAction="#txtUserIdOnAction" promptText="Enter User ID" />
      <TextField fx:id="txtVehicleId" layoutX="38.0" layoutY="120.0" promptText="Vehicle Id" />
      <TextField fx:id="txtVehicleBrand" layoutX="38.0" layoutY="162.0" promptText="Vehicle Brand" />
      <ComboBox fx:id="cmbVehicleType" layoutX="38.0" layoutY="207.0" prefWidth="150.0" promptText="Vehicle Type" />
      <TableView fx:id="tblParkingDetails" layoutX="258.0" layoutY="45.0" prefHeight="349.0" prefWidth="326.0">
        <columns>
          <TableColumn fx:id="colParkingDate" prefWidth="157.0" text="Parking Date" />
          <TableColumn fx:id="colCost" prefWidth="168.0" text="Cost" />
        </columns>
      </TableView>
      <Button layoutX="38.0" layoutY="284.0" mnemonicParsing="false" onAction="#btnSaveOnAction" prefHeight="25.0" prefWidth="63.0" text="Save" />
      <Button layoutX="140.0" layoutY="284.0" mnemonicParsing="false" onAction="#btnUpdateOnAction" prefHeight="25.0" prefWidth="63.0" text="Update" />
      <Button layoutX="38.0" layoutY="332.0" mnemonicParsing="false" onAction="#btnDeleteOnAction" prefHeight="25.0" prefWidth="63.0" text="Delete" />
      <Button layoutX="140.0" layoutY="332.0" mnemonicParsing="false" onAction="#btnClearOnAction" prefHeight="25.0" prefWidth="63.0" text="Clear" />
   </children>
</AnchorPane>
